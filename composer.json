{
    "name": "markup/wistia-bundle",
    "description": "The Markup Wistia Bundle contains functionality for communicating with the Wistia Data API",
    "keywords": ["media", "markup", "wistia", "video"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Calum Brodie",
            "email": "calum@usemarkup.com"
        },
        {
            "name": "Markup",
            "homepage": "http://www.usemarkup.com/"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.1",
        "twig/twig": ">=1.12",
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": ">=4.0",
        "psr/cache": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "~0.9"
    },
    "suggests": {
        "symfony/console": "To perform operations using the console"
    },
    "autoload": {
        "psr-4": { "Markup\\WistiaBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
